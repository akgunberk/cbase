{
  "name": "cbase",
  "version": "1.0.0",
  "description": "Bring codabasehq into your terminal",
  "keywords": [
    "cli",
    "codebasehq"
  ],
  "author": "Berk Akgun",
  "license": "ISC",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "type": "commonjs",
  "bin": {
    "cbase": "lib/index.js"
  },
  "publishConfig": {
    "@YOUR-USERNAME:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "test": "jest --watch",
    "type-check": "tsc --noEmit",
    "postinstall": "npm run prisma",
    "prisma": "npx prisma migrate dev --name initial-migration",
    "dev": "nodemon --watch src -e ts --exec \"npm run build\"",
    "build1": "tsc && babel src --out-dir lib --extensions \".ts,.js,.json\"",
    "watch": "parcel watch",
    "build": "parcel build"
  },
  "devDependencies": {
    "@types/inquirer": "^8.1.3",
    "@types/node": "^17.0.9",
    "@types/xml2js": "^0.4.9",
    "typescript": "^4.5.4",
    "parcel": "latest"
  },
  "dependencies": {
    "@prisma/client": "^3.9.1",
    "axios": "^0.24.0",
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "dotenv": "^14.2.0",
    "inquirer": "^8.2.0",
    "ora": "^5.1.0",
    "xml-js": "^1.6.11"
  }
}
